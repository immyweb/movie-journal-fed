{
  "scripts": {
    "build": "webpack --config webpack.prod.config.ts",
    "dev": "npm-run-all --parallel mockapi start",
    "mockapi": "json-server --watch src/data/db.json",
    "start": "webpack serve --config webpack.dev.config.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "test:e2e:dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-test mockapi 3000 build 4000 cy:run",
    "test:e2e:dev": "start-test mockapi 3000 start 4000 cy:open",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx|css|html|md)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint test test:e2e:run"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@testing-library/cypress": "^7.0.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.5.0",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^26.0.24",
    "@types/jest-axe": "^3.5.3",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.0",
    "cypress": "^7.7.0",
    "eslint": "^7.20.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "history": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.3.1",
    "jest-axe": "^5.0.1",
    "lint-staged": "^11.2.6",
    "mini-css-extract-plugin": "^2.4.3",
    "msw": "^0.35.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "whatwg-fetch": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged && npm run test:e2e:run"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.+(js|json|ts|tsx|css|html|md)": [
      "prettier",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/src/__mocks__/fileMock.ts"
    },
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx,js}"
    ],
    "resetMocks": true,
    "testEnvironment": "jsdom"
  },
  "name": "movie-journal-front-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "",
  "license": "ISC"
}
